<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp"
    tools:context="com.ks.seal.launcher.MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/v1"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/seal_bg_style_fang_xing_yang_ke_yin_zhang"
            android:text="玉传玺国"
            android:textAlignment="center"
            android:textColor="#ff0000"
            android:textSize="39dp" />

        <TextView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginLeft="20dp"
            android:background="@drawable/seal_bg_style_fang_xing_yin_ke_yin_zhang"
            android:text="玉传玺国"
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:textSize="39dp" />

        <TextView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginLeft="20dp"
            android:background="@drawable/seal_bg_style_fang_xing_yang_ke_yuan_jiao"
            android:text="玉传玺国"
            android:textAlignment="center"
            android:textColor="#ff0000"
            android:textSize="39dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:layout_marginTop="10dp">

        <TextView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/seal_bg_style_fang_xing_yin_ke_yuan_jiao"
            android:text="玉传玺国"
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:textSize="39dp" />

        <TextView
            android:layout_marginLeft="20dp"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/seal_bg_style_yuan_xing_yang_ke_yin_zhang"
            android:text="玉传玺国"
            android:textAlignment="center"
            android:textColor="#ff0000"
            android:paddingTop="12dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingBottom="10dp"
            android:textSize="30dp" />

        <TextView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginLeft="20dp"
            android:background="@drawable/seal_bg_style_yuan_xing_yin_ke_yin_zhang"
            android:text="玉传玺国"
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:paddingTop="12dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingBottom="10dp"
            android:textSize="30dp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:layout_marginTop="10dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/seal_bg_style_fang_xing_yang_ke_yin_zhang"
            android:text="中书令"
            android:textAlignment="center"
            android:padding="10dp"
            android:layout_gravity="center_vertical"
            android:textColor="#ff0000"
            android:textSize="29dp" />

        <TextView
            android:layout_marginLeft="20dp"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/seal_bg_style_yuan_xing_yang_ke_yin_zhang"
            android:text="玉传玺国"
            android:textAlignment="center"
            android:textColor="#ff0000"
            android:paddingTop="12dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingBottom="10dp"
            android:textSize="30dp" />

        <TextView
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginLeft="20dp"
            android:background="@drawable/seal_bg_style_yuan_xing_yin_ke_yin_zhang"
            android:text="玉传玺国"
            android:textAlignment="center"
            android:textColor="#ffffff"
            android:paddingTop="12dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingBottom="10dp"
            android:textSize="30dp" />
    </LinearLayout>
</LinearLayout>
